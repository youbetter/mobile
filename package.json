{
  "name": "youbetter-mobile-app",
  "version": "0.1.0",
  "description": "You Better for mobile devices",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youbetter/mobile-app.git"
  },
  "keywords": [
    "youbetter",
    "shine",
    "goals",
    "training",
    "mobile",
    "app"
  ],
  "author": "Sean Jacke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/youbetter/mobile-app/issues"
  },
  "homepage": "http://youbetter.today",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-hmr",
                  "imports": [
                    "react"
                  ],
                  "locals": [
                    "module"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "phonegap-facebook-plugin": "git+https://github.com/Wizcorp/phonegap-facebook-plugin.git",
    "pouchdb": "^5.1.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-transform-hmr": "^1.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "start": "NODE_ENV=development webpack-dev-server --progress --inline --hot --content-base build/"
  }
}
